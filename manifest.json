{
  "manifest_version": 2,
  "name": "DX",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-3.1.1.min.js", "content.js"]
  }],


  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },

  "browser_action": {
    "default_title": "DX",
    "default_popup": "popup.html"
  },


  "permissions": [
    "activeTab",
    "<all_urls>"
  ]

}
